{
  "name": "api",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "lint:fix": "eslint --ext .js,.ts ./src --fix",
    "build": "tsc",
    "type-check": "echo '# api type-check' && tsc --noEmit --diagnostics",
    "serve": "tsc-watch --onFirstSuccess 'firebase emulators:start --only functions'",
    "shell": "yarn build && firebase functions:shell",
    "start": "yarn shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "generate-schema": "npx prisma@5.19.1 generate",
    "gcp-build": "yarn generate-schema",
    "plugin": "echo dummyCommandForAvoidingFirebaseError"
  },
  "engines": {
    "node": "20"
  },
  "main": "lib/index.js",
  "volta": {
    "node": "20.9.0",
    "yarn": "4.0.2"
  },
  "dependencies": {
    "@prisma/client": "5.19.1",
    "@slack/bolt": "3",
    "@slack/web-api": "^6.2.4",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "firebase-admin": "^10.0.2",
    "firebase-functions": "^6.0.1",
    "openai": "^5.22.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "typescript": "*"
  }
}
