{
  "name": "slackbot",
  "version": "2.1.1",
  "description": "slack bot for oogiri",
  "main": "index.js",
  "author": "tocomi",
  "license": "MIT",
  "scripts": {
    "start": "node ./dist/index.js",
    "watch": "tsc-watch --onSuccess 'pnpm start'",
    "lint": "eslint --ext .js,.ts ./src",
    "lint:fix": "eslint --ext .js,.ts ./src --fix",
    "test": "jest",
    "tsc": "tsc",
    "deploy:heroku": "pnpm tsc && pnpm start"
  },
  "packageManager": "pnpm@10.15.1",
  "pnpm": {
    "executionEnv": {
      "nodeVersion": "22.19.0"
    }
  }
}
